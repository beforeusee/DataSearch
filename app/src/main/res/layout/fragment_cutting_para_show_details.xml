                        android:background="@color/light1_blue"/>

                    <EditText
                        android:id="@+id/et_cutting_para_feedRate"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="end"
                        android:padding="0dp"
                        android:layout_margin="0.5dp"
                        android:textSize="20sp"
                        android:background="@color/white"/>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/dark_white"
                    android:layout_margin="0.5dp"
                    >

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="start"
                        android:text="径向切宽(mm)"
                        android:textSize="20sp"
                        android:padding="0dp"
                        android:layout_margin="0.5dp"
                        android:background="@color/dark_white"/>

                    <EditText
                        android:id="@+id/et_cutting_para_cutting_width"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="end"
                        android:textSize="20sp"
                        android:padding="0dp"
                        android:layout_margin="0.5dp"
                        android:background="@color/white"/>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="0.5dp">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="start"
                        android:text="轴向切深(mm)"
                        android:textSize="20sp"
                        android:padding="0dp"
                        android:layout_margin="0.5dp"
                        android:background="@color/light1_blue"/>

                    <EditText
                        android:id="@+id/et_cutting_para_cutting_depth"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="end"
                        android:padding="0dp"
                        android:layout_margin="0.5dp"
                        android:textSize="20sp"
                        android:background="@color/white"/>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/dark_white"
                    android:layout_margin="0.5dp"
                    >

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="start"
                        android:text="每齿进给量(mm/z)"
                        android:textSize="20sp"
                        android:padding="0dp"
                        android:layout_margin="0.5dp"
                        android:background="@color/dark_white"/>

                    <EditText
                        android:id="@+id/et_cutting_para_feed_per_tooth"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="end"
                        android:textSize="20sp"
                        android:padding="0dp"
                        android:layout_margin="0.5dp"
                        android:background="@color/white"/>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="0.5dp">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="start"
                        android:text="切削线速度(m/min)"
                        android:textSize="20sp"
                        android:padding="0dp"
                        android:layout_margin="0.5dp"
                        android:background="@color/light1_blue"/>

                    <EditText
                        android:id="@+id/et_cutting_para_cutting_linear_velocity"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="end"
                        android:padding="0dp"
                        android:layout_margin="0.5dp"
                        android:textSize="20sp"
                        android:background="@color/white"/>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/dark_white"
                    android:layout_margin="0.5dp"
                    >

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="start"
                        android:text="Y向切削力峰值(N)"
                        android:textSize="20sp"
                        android:padding="0dp"
                        android:layout_margin="0.5dp"
                        android:background="@color/dark_white"/>

                    <EditText
                        android:id="@+id/et_cutting_para_max_force_y"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="end"
                        android:textSize="20sp"
                        android:padding="0dp"
                        android:layout_margin="0.5dp"
                        android:background="@color/white"/>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="0.5dp">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="start"
                        android:text="XY向切削力峰值(N)"
                        android:textSize="20sp"
                        android:padding="0dp"
                        android:layout_margin="0.5dp"
                        android:background="@color/light1_blue"/>

                    <EditText
                        android:id="@+id/et_cutting_para_max_force_xy"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="end"
                        android:padding="0dp"
                        android:layout_margin="0.5dp"
                        android:textSize="20sp"
                        android:background="@color/white"/>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/dark_white"
                    android:layout_margin="0.5dp"
                    >

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="start"
                        android:text="切向力峰值(N)"
                        android:textSize="20sp"
                        android:padding="0dp"
                        android:layout_margin="0.5dp"
                        android:background="@color/dark_white"/>

                    <EditText
                        android:id="@+id/et_cutting_para_max_force_tangential"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="end"
                        android:textSize="20sp"
                        android:padding="0dp"
                        android:layout_margin="0.5dp"
                        android:background="@color/white"/>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="0.5dp">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
